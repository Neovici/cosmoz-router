{
	"name": "@neovici/cosmoz-page-router",
	"version": "8.1.2",
	"description": "A Polymer component to handle client side URL routing and view loading / management",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-page-router#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-page-router/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-page-router.git"
	},
	"license": "Apache-2.0",
	"author": "",
	"main": "cosmoz-page-router.js",
	"directories": {
		"test": "test"
	},
	"files": [
		"lib/",
		"cosmoz-*.js"
	],
	"scripts": {
		"lint": "tsc && eslint --cache .",
		"build": "tsc -p tsconfig.build.json",
		"start": "wds",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch",
		"prepare": "husky install"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master",
		"preset": "conventionalcommits"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"dependencies": {
		"@neovici/cosmoz-utils": "^3.25.0",
		"haunted": "^4.7.0",
		"lit-html": "^1.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@neovici/cfg": "github:Neovici/cfg#feat/typescript",
		"@open-wc/testing": "^2.5.16",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@web/dev-server": "^0.1.28",
		"@web/dev-server-storybook": "^0.5.1",
		"@web/test-runner": "^0.13.23",
		"@webcomponents/webcomponentsjs": "^2.4.3",
		"husky": "^8.0.0",
		"semantic-release": "^19.0.0",
		"sinon": "^14.0.0"
	}
}
